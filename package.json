{
  "name": "museeker-www",
  "version": "0.0.1",
  "author": "Jos√© Manuel Aguirre",
  "license": "MIT",
  "scripts": {
    "update-jmaguirrei": "ncu -u '/^@jmaguirrei.*$/'",

    "build": "NODE_ENV=production parcel build ./src/client/main.js -o main.js",

    "git-i1": "git add .",
    "git-i2": "git diff-index --quiet HEAD || git commit -m 'Initial commit'",
    "git-i3": "git push",
    "git-initial": "npm run git-i1 && npm run git-i2 && npm run git-i3",

    "deploy": "npm run update-jmaguirrei && npm run build && npm run git-initial",

    "browser": "parcel --no-hmr ./src/client/main.js -o main.js",
    "start-dev": "nodemon start-dev.js",
    "start-prod": "node start-prod.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0"
  },
  "dependencies": {
    "@jmaguirrei/belt": "0.0.18",
    "@jmaguirrei/server": "0.0.72",
    "@sendgrid/mail": "^6.3.1"
  }
}
